{
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "browser": true,
    "mocha": true,
    "node": true
  },
  "rules": {
    "arrow-body-style": [0],
    "eol-last": [0],
    "global-require": [0],
    "no-mixed-requires": [0],
    "no-return-assign": [0],
    "no-underscore-dangle": [0],
    "no-use-before-define": [0],
    "no-unused-vars": [0],
    "quotes": [2, "single"]
  }
}
